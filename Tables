Create table WareHouse(
  WH_ID   int primary key,
  War_name varchar 
);

Create table ID_map(
 Inv_ID  int primary key,
 WH_ID   int,
 Prod_ID varchar 
);

Create table Products(
  Prod_ID   int primary key,
  Prod_name varchar
);

Create table Bye_Orders(
  Prod_amu int,
  Inv_ID int PRIMARY KEY,
    
   FOREIGN KEY (Inv_ID) REFERENCES WH_inv(Inv_ID)
);

Create table Sell_Orders(
  Amount int,
  Inv_ID int PRIMARY KEY,

   FOREIGN KEY (Inv_ID) REFERENCES WH_inv(Inv_ID)
);

Create table WH_inv(
  Inv_ID     int primary key,
  Amount  Bigint
);
